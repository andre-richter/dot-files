# Set CLICOLOR if you want Ansi Colors in iTerm2
export CLICOLOR=1

# Set colors to match iTerm2 Terminal Colors
export TERM=xterm-256color

alias ls='ls -G'

alias agent-reload='sudo killall gpg-agent && gpg-agent --daemon'
export GPG_TTY=$(tty)

source ~/repos/dot-files/.bashrc

gpg-agent --daemon
